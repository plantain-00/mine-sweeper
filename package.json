{
  "name": "mine-sweeper",
  "version": "1.0.0",
  "description": "A mine sweeper game.",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "tslint": "tslint \"index.ts\"",
    "lessc": "lessc index.less > index.css",
    "stylelint": "stylelint \"index.less\"",
    "cleancss": "cleancss -o index.min.css index.css ./node_modules/github-fork-ribbon-css/gh-fork-ribbon.css",
    "clean": "rimraf dist",
    "file2variable": "file2variable-cli index.template.html -o variables.ts --html-minify",
    "sw-precache": "sw-precache --config sw-precache.config.js --verbose && uglifyjs service-worker.js -o service-worker.bundle.js",
    "webpack": "webpack --config webpack.config.js",
    "rev-static": "rev-static --config rev-static.config.js",
    "clean-rev": "rimraf index.min-*.js index.min-*.css",
    "build": "npm run clean-rev && npm run clean && npm run file2variable && npm run tsc && npm run lessc && npm run cleancss && npm run webpack && npm run rev-static && npm run sw-precache",
    "lint": "npm run tslint && npm run stylelint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/mine-sweeper.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/mine-sweeper/issues"
  },
  "homepage": "https://github.com/plantain-00/mine-sweeper#readme",
  "devDependencies": {
    "clean-css-cli": "4.1.5",
    "file2variable-cli": "1.3.0",
    "github-fork-ribbon-css": "0.2.1",
    "less": "2.7.2",
    "rev-static": "2.4.2",
    "rimraf": "2.6.1",
    "stylelint": "7.11.1",
    "stylelint-config-standard": "16.0.0",
    "sw-precache": "5.2.0",
    "tslib": "1.7.1",
    "tslint": "5.4.3",
    "typescript": "2.4.0",
    "vue": "2.3.4",
    "vue-class-component": "5.0.1",
    "webpack": "2.6.1"
  }
}
